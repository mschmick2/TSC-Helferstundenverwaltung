# =============================================================================
# VAES - Schutz für /storage Verzeichnis
# Enthält Logs, Cache und temporäre Dateien - nicht öffentlich zugänglich!
# =============================================================================

# Apache 2.4+
<IfModule mod_authz_core.c>
    Require all denied
</IfModule>

# Apache 2.2 (Fallback)
<IfModule !mod_authz_core.c>
    Order allow,deny
    Deny from all
</IfModule>
