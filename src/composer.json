{
    "name": "vaes/helferstundenverwaltung",
    "description": "VAES - Vereins-Arbeitsstunden-Erfassungssystem",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.1",
        "chillerlan/php-qrcode": "^5.0",
        "monolog/monolog": "^3.5",
        "php-di/php-di": "^7.0",
        "php-di/slim-bridge": "^3.4",
        "phpmailer/phpmailer": "^6.9",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.12",
        "spomky-labs/otphp": "^11.0",
        "tecnickcom/tcpdf": "^6.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "../tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
